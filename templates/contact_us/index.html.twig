{% extends 'base.html.twig' %}

{% block title %}Terre d'Avalonne - Contacter-nous{% endblock %}

{% block body %}

<div class="contact-us-wrapper">
    {#              HEAD              #}
    {{ include('template/_head_page.html.twig', {title: 'Contacter nous'}) }}
    {#              FORM CONTACT US              #}
    <div class="contact-us-container">
        <div class="bg-white p-5 contact-us-form">
            {{ form_start(form) }}
            <div class="form-group">
                {{ form_row(form.theme, {'attr': {'class': 'form-control'}}) }}
            </div>
            <div class="form-group">
            {{ form_row(form.name, {'attr': {'class': 'form-control'}}) }}
            </div>
            <div class="form-group">
                {{ form_row(form.email, {'attr': {'class': 'form-control'}}) }}
            </div>
            <div class="form-group">
                {{ form_row(form.message, {'attr': {'class': 'form-control'}}) }}
            </div>

            <div class="mt-30 form-group row justify-content-center">
				<button type="submit" class="px-5 w-50-100 btn btn-primary py-3">Envoyer</button>
            </div>
            {{ form_end(form) }}
        </div>
    </div>
    <div style="text-align: center">
        <svg width="353" height="347" viewBox="0 0 353 347" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
            <rect width="353" height="347" fill="url(#pattern0)"/>
            <defs>
                <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                    <use xlink:href="#image0_319_5622" transform="translate(0 -0.00864553) scale(0.00444444)"/>
                </pattern>
                <image id="image0_319_5622" width="225" height="225" xlink:href="data:image/jpeg;base64,"/>
            </defs>
        </svg>
    </div>
</div>
{% endblock %}
